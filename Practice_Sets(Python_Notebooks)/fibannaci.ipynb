    "def fibnew(n): \n",
    "    if n <= 1: \n",
    "        return n \n",
    "    a, b = 0, 1 \n",
    "    \n",
    "    for i in range(1, n+1):\n",
    "        a, b = b, a + b\n",
    "        print(a,b)\n",
    "    return a"
   ]
